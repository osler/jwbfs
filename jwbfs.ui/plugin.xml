            commandId="it.jwbfs.core.toWbfs">
      </handler>
      <handler
            class="jwbfs.ui.handlers.ToISOConvertHandler"
            commandId="it.jwbfs.core.toIso">
      </handler>
      <handler
            class="jwbfs.ui.handlers.CheckDiscHandler"
            commandId="it.jwbfs.core.checkDisk">
      </handler>
      <handler
