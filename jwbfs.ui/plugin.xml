            id="it.jwbfs.ui.DiskView4"
            name="Disk 4"
            restorable="true">
      </view>
      <view
            class="jwbfs.ui.views.manager.ManagerView5"
            icon="icons/drive-removable-media-usb.png"
            id="it.jwbfs.ui.DiskView5"
            name="Disk 5"
            restorable="true">
      </view>
      <view
            class="jwbfs.ui.views.manager.ManagerView6"
            icon="icons/drive-removable-media-usb.png"
            id="it.jwbfs.ui.DiskView6"
            name="Disk 6"
            restorable="true">
      </view>      
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="jwbfs.ui.commands.category.convert"
            name="Convert">
      </category>
      <command
            categoryId="jwbfs.ui.commands.category.convert"
            id="it.jwbfs.core.toWbfs"
            name="Iso To wbfs">
      </command>
      <command
            categoryId="jwbfs.ui.commands.category.convert"
            id="it.jwbfs.core.toIso"
            name="Wbfs To Iso">
      </command>
      <command
            categoryId="jwbfs.ui.commands.category.convert"
            id="it.jwbfs.core.checkDisk"
            name="Check Disk">
            <commandParameter 
        		id="wbfs" 
        		name="wbfs" 
        		optional="false">
        	</commandParameter>  
      </command>
      <command
            categoryId="jwbfs.ui.commands.category.convert"
            id="it.jwbfs.core.fileDialog"
            name="File Dialog">
      </command>
      <command
            categoryId="jwbfs.ui.commands.category.convert"
            id="it.jwbfs.core.fileDialogExport"
            name="File Dialog Export">
      </command>
      <command
            categoryId="jwbfs.ui.commands.category.convert"
            id="it.jwbfs.core.folderDialog"
            name="Folder Dialog">
      </command>
            <command
            categoryId="jwbfs.ui.commands.category.convert"
            id="it.jwbfs.core.folderCoverDialog"
            name="Folder Cover Dialog">
      </command>
     <command
            id="it.jwbfs.core.settings.save"
            name="Save Settings">
      </command>      
      <command
            id="it.jwbfs.core.updateCover"
            name="Update Cover">
             <commandParameter 
        		id="diskID" 
        		name="diskID" 
        		optional="false">
        	</commandParameter>            
      </command>
      <command
            id="it.jwbfs.core.updateCoverForced"
            name="update Cover Forced">
             <commandParameter 
        		id="diskID" 
        		name="diskID" 
        		optional="false">
        	</commandParameter>            
      </command>
      <command
            id="it.jwbfs.core.game.add"
            name="Add game">
      </command>
      <command
            id="it.jwbfs.core.game.export"
            name="Export game">
      </command>
      <command
            id="it.jwbfs.core.game.delete"
            name="Delete game">
      </command>                 
      <command
            id="it.jwbfs.core.updateGameList"
            name="Update Game List">
             <commandParameter 
        		id="diskID" 
        		name="diskID" 
        		optional="false">
        	</commandParameter>
      </command>
      <command
            id="it.jwbfs.core.updateTitles"
            name="Update Titles TXT">
      </command>
      <command
            id="it.jwbfs.core.deleteFile"
            name="Delete File">
      </command>
      <command
            id="it.jwbfs.core.exit"
            name="Exit">
      </command>  
      <command
            id="it.jwbfs.core.refreshFirstDisk"
            name="Refresh First Disk">
             <commandParameter 
        		id="diskID" 
        		name="diskID" 
        		optional="false">
        	</commandParameter>
      </command> 
      <command
            id="it.jwbfs.core.refreshAllDisks"
            name="Refresh all disks">
      </command>       
      <command
            id="it.jwbfs.core.changeDisksPerspective"
            name="Change disks perspective">
             <commandParameter 
        		id="numDisks" 
        		name="numDisks" 
        		optional="false">
        	</commandParameter>
      </command> 
      <command
            id="it.jwbfs.core.games.cut"
            name="Cut Games">
             <commandParameter 
        		id="diskFrom" 
        		name="diskFrom" 
        		optional="false">
        	</commandParameter>
             <commandParameter 
        		id="diskTo" 
        		name="diskTo" 
        		optional="false">
        	</commandParameter>
      </command>   
     <command
            id="it.jwbfs.core.games.copy"
            name="Copy Games">
             <commandParameter 
        		id="diskFrom" 
        		name="diskFrom" 
        		optional="false">
        	</commandParameter>
             <commandParameter 
        		id="diskTo" 
        		name="diskTo" 
        		optional="false">
        	</commandParameter>
      </command> 
     <command
            id="it.jwbfs.core.games.copy.paste"
            name="Copy Games">
             <commandParameter 
        		id="diskFrom" 
        		name="diskFrom" 
        		optional="false">
        	</commandParameter>
             <commandParameter 
        		id="diskTo" 
        		name="diskTo" 
        		optional="false">
        	</commandParameter>
      </command>    
     <command
            id="it.jwbfs.core.games.paste"
            name="Paste Games">
             <commandParameter 
        		id="diskFrom" 
        		name="diskFrom" 
        		optional="false">
        	</commandParameter>
             <commandParameter 
        		id="diskTo" 
        		name="diskTo" 
        		optional="false">
        	</commandParameter>
      </command>                  
   </extension>
   
